<odoo>
    <record id="view_stock_movement_analysis_wizard" model="ir.ui.view">
        <field name="name">stock.movement.analysis.wizard.form</field>
        <field name="model">stock.movement.analysis.wizard</field>
        <field name="arch" type="xml">
            <form string="Stock Movement Analysis">
                <group>
                    <field name="product_id"/>
                    <field name="categ_id"/>
                    <field name="warehouse_id"/>
                    <field name="location_id"/>
                    <field name="company_id"/>
                    <field name="date_start"/>
                    <field name="date_end"/>
                </group>
                <footer>
                    <button name="action_generate_report"
                            type="object"
                            string="Generate"
                            class="btn-primary"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>